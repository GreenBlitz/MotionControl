package APPC;

import java.util.ArrayList;

public class PathGenerator {
	
	public static ArrayList<Point2D> genStraightLine(Point2D origin, double len, double metersPerPoint, double rotation){
		ArrayList<Point2D> path = new ArrayList<>();
		for(int i = 0; i <= len; i += metersPerPoint)
		return path;
	}
	
}
