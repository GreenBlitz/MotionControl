package base;

public class Event {
	
	
	private boolean m_isCanceled;
	private boolean m_isCancelable;
	private long m_timestamp;
	
	
	public boolean isCancelable(){
		return m_isCancelable;
	}
	
	public boolean isCanceled(){
		return m_isCanceled;
	}
	
	public void setCanceled(boolean canceled){
		isCanceled = true;
	}
}
